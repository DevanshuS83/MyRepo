<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddServlet</servlet-name>
  	<display-name>AddServlet</display-name>
  	<description></description>
  	<servlet-class>com.dd.controller.AddServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Delete1Servlet</servlet-name>
  	<display-name>Delete1Servlet</display-name>
  	<description></description>
  	<servlet-class>com.dd.controller.Delete1Servlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Delete2Servlet</servlet-name>
  	<display-name>Delete2Servlet</display-name>
  	<description></description>
  	<servlet-class>com.dd.controller.Delete2Servlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.dd.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.dd.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ModifyServlet</servlet-name>
  	<display-name>ModifyServlet</display-name>
  	<description></description>
  	<servlet-class>com.dd.controller.ModifyServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SearchServlet</servlet-name>
  	<display-name>SearchServlet</display-name>
  	<description></description>
  	<servlet-class>com.dd.controller.SearchServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SearchAllServlet</servlet-name>
  	<display-name>SearchAllServlet</display-name>
  	<description></description>
  	<servlet-class>com.dd.controller.SearchAllServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddServlet</servlet-name>
  	<url-pattern>/addEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Delete1Servlet</servlet-name>
  	<url-pattern>/proceedDelete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Delete2Servlet</servlet-name>
  	<url-pattern>/confirmDelete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ModifyServlet</servlet-name>
  	<url-pattern>/modifyEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearchServlet</servlet-name>
  	<url-pattern>/searchEmployee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearchAllServlet</servlet-name>
  	<url-pattern>/searchAll</url-pattern>
  </servlet-mapping>
</web-app>
